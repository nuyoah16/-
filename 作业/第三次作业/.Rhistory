loss <- function(par, y, x)
{
beta0 <- par[1]
beta1 <- par[2]
yhat <- beta0+beta1*x
error=(yhat-y)^2
lossfunction=sum(error)/length(x)
return(lossfunction)
}
x <- rnorm(n, 3, 1)
y <- 1+x*3
optimOut <- optim(c(0.2, 0.3), loss,
x = x, y = y)
